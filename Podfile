platform :ios, '8.0'
source 'https://github.com/CocoaPods/Specs.git'
xcodeproj 'FlickeringLights.xcodeproj'
#use_frameworks!

# ignore all warnings from all pods
inhibit_all_warnings!


pod 'SDWebImage', '3.8.0'                   # see https://github.com/rs/SDWebImage                          - Asynchronous image downloader with cache support as a UIImageView category
pod 'JSONModel', '1.2.0'                    # see https://github.com/jsonmodel/jsonmodel                    - Data Modelling Framework for JSON
pod 'UIImage+MDContentColor', '1.0.1'       # see https://github.com/mdjordjevic/uiimage-content-color      - UIImage category for getting average color